{
    "version": 2,
    "name": "nlrc-homework-solver",
    "framework": null,
    "buildCommand": null,
    "devCommand": null,
    "installCommand": null,
    "outputDirectory": ".",
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico|json))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}